{
	"extends": "eslint",
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module"
	},
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	},
	"globals": {
		"TimelineLite" : false,
		"TimelineMax" : false,
		"TweenLite" : false,
		"TweenMax" : false,
		"Back" : false,
		"Bounce" : false,
		"Circ" : false,
		"Cubic" : false,
		"Ease" : false,
		"EaseLookup" : false,
		"Elastic" : false,
		"Expo" : false,
		"Linear" : false,
		"Power0" : false,
		"Power1" : false,
		"Power2" : false,
		"Power3" : false,
		"Power4" : false,
		"Quad" : false,
		"Quart" : false,
		"Quint" : false,
		"RoughEase" : false,
		"Sine" : false,
		"SlowMo" : false,
		"SteppedEase" : false,
		"Strong" : false,
		"Draggable" : false,
		"SplitText" : false,
		"VelocityTracker" : false,
		"CSSPlugin" : false,
		"ThrowPropsPlugin" : false,
		"BezierPlugin" : false
	},
	"rules": {
		"arrow-body-style": 0,
		"comma-dangle": ["error", "always-multiline"],
		"func-style": 0,
		"indent": ["error", "tab"],
		"lines-around-comment": 0,
		"no-tabs": 0,
		"prefer-template": 0,
		"quotes": ["error", "single"],
		"require-jsdoc": 0,
		"semi": ["error", "never"]
	}
}
